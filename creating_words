#include <bits/stdc++.h>
using namespace std;
#define fastread()      (ios_base:: sync_with_stdio(false),cin.tie(NULL));

int main() {
    fastread();
    int t;
    cin>>t;
    vector <pair<string,string>> word(t);
    for(auto &it : word){
        cin>>it.first>>it.second;    
        swap(it.first[0],it.second[0]);    
    }


    for(auto &it : word){
        cout<<it.first<<" "<<it.second<<"\n";
    }    
    return 0;
}
