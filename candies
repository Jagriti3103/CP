#include <bits/stdc++.h>
using namespace std;

int main(){
    int n, start,end,sum=0;
    cin>>n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cin>>arr[i];
    }
    cin>>start>>end;
    for (int i = start; i <= end; i++)
    {
        sum+= arr[i];
    }
    cout<<sum;
    return 0;
}
